{
  "canisters": {
    "user": {
      "candid": "src/user/user.did",
      "package": "user",
      "type": "custom",
      "wasm": "target/wasm32-wasi/release/user-ic.wasm",
      "build": [ "bash build.sh" ]
    },
    "CDTP": {
      "type": "custom",
      "candid": "src/icrc_ledger/icrc_ledger.did",
      "wasm": "src/icrc_ledger/icrc_ledger.wasm"
    },
    "staking":{
      "type":"motoko",
      "main":"src/staking/staking.mo"
    },
    "frontend": {
      "dependencies": [
        "user"
      ],
      "source": [
        "src/frontend/dist"
      ],
      "type": "assets",
      "workspace": "frontend"
    },
    "invoice" : {
      "main": "src/backend_mo/invoice.mo",
      "type": "motoko"
    },
    "loan" : {
      "main": "src/backend_mo/loan.mo",
      "type": "motoko"
    },
    "wallet" : {
      "main": "src/backend_mo/wallet.mo",
      "type": "motoko",
      "dependencies": ["icp_ledger_canister"]
    },
    "icp_ledger_canister": {
            "type": "custom",
            "candid": "https://raw.githubusercontent.com/dfinity/ic/d87954601e4b22972899e9957e800406a0a6b929/rs/rosetta-api/icp_ledger/ledger.did",
            "wasm": "https://download.dfinity.systems/ic/d87954601e4b22972899e9957e800406a0a6b929/canisters/ledger-canister.wasm.gz",
            "remote": {
                "id": {
                    "ic": "ryjl3-tyaaa-aaaaa-aaaba-cai"
                }
            }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}