{
  "canisters": {
    "backend_mo": {
      "main": "src/backend_mo/main.mo",
      "type": "motoko"
    },
    "user": {
      "candid": "src/user/user.did",
      "package": "user",
      "type": "custom",
      "wasm": "target/wasm32-wasi/release/user-ic.wasm",
      "build": [ "bash build.sh" ]
    },
    "frontend": {
      "dependencies": [
        "backend_mo"
      ],
      "source": [
        "src/frontend/dist"
      ],
      "type": "assets",
      "workspace": "frontend"
    },
    "ledger": {
      "type": "custom",
      "candid": "src/ledger/ledger.did",
      "wasm": "src/ledger/ledger.wasm"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}