{
  "canisters": {
    "ctICP": {
      "main": "src/DIP20/motoko/src/token.mo",
      "type": "motoko"
    },
    "backend_rust": {
      "candid": "src/backend_rust/backend_rust.did",
      "package": "backend_rust",
      "type": "custom",
      "wasm": "target/wasm32-wasi/release/backend_rust-ic.wasm",
      "build": [ "bash build.sh" ]
    },
    "backend_azle": {
      "build": "npx azle backend_azle",
      "candid": "src/backend_azle/backend_azle.did",
      "gzip": true,
      "main": "src/backend_azle/src/index.ts",
      "tech_stack": {
        "cdk": {
          "azle": {}
        },
        "language": {
          "javascript": {},
          "typescript": {}
        }
      }
    },
    "ledger": {
      "type": "custom",
      "candid": "src/ledger/ledger.did",
      "wasm": "src/ledger/ledger.wasm"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}